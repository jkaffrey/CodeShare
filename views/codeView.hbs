<!-- <h5>Your unique room id is: <span id="roomName"></span></h5> -->
<div class="row">
  <div class="col-md-3">
    <div class="row fileTree panel panel-body">
      <span>Project: {{directory}}</span>
      <div id="fileTree"></div>
      <hr />
      <button class="btn btn-primary" onclick="createFile()">[+ File] </button><br />
      <button class="btn btn-success" onclick="createFolder()">[+ Folder]</button><br />
      <button class="btn btn-info" onclick="rename()">[Rename]</button><br />
      <button class="btn btn-danger" onclick="deleteFunc()">[-]</button>
    </div>
    <div class="row connectedUsers panel panel-body">
      <span>Connected Users</span>
      <ul class="users">
      </ul>
    </div>
  </div>
  <div class="col-md-9">
    <pre>
      <code contenteditable="true" id="codeArea">
        function helloWorld() {

          console.log('Hello World!');
        }

        hellowWorld();
      </code>
    </pre>
  </div>
</div>
